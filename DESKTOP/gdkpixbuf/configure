export LDFLAGS="-L/usr/pkgs/libjpeg/lib"
export CPPFLAGS="-I/usr/pkgs/libjpeg/include"

./configure --prefix=$PREFIX 		\
            --enable-debug=no		\
            --datarootdir=$DATAROOTDIR 	\
            --with-x11 			\
            --disable-nls 		\
	    --disable-glibtest		\
            --disable-gtk-doc

