
export CFLAGS="-I/usr/pkgs/cups/include"
export LDFLAGS="-L/usr/pkgs/cups/lib"


./configure --prefix=$PREFIX         	\
	    --build=$LMBX_BUILD		\
            --enable-debug=no         	\
            --enable-gtk2-dependency  	\
            --enable-xkb              	\
            --enable-xinerama         	\
            --enable-xrandr           	\
            --disable-gtk-doc         	\
            --enable-broadway-backend 	\
            --enable-x11-backend      	\
            --disable-wayland-backend 
